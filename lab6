#include <iostream>
#include <math.h>


using namespace std;
double A,B,k;

int main()
{
	setlocale(LC_ALL, "Russian");
	cout << "Введите A=";
	cin >> A;
	cout << "Введите B=";
	cin >> B;
	k = A;
	A = B;
	B = k;
	cout << "A=" << A << "; B=" << B;
	return 0;
}
2.
#include <iostream>
#include <math.h>


using namespace std;
double A,B,C,k;

int main()
{
	setlocale(LC_ALL, "Russian");
	cout << "Введите A=";
	cin >> A;
	cout << "Введите B=";
	cin >> B;
	cout << "Введите C=";
	cin >> C;
	k = A;
	A = B;
	B = k;
	k = A;
	A = C;
	C = k;
	cout << "A=" << A << "; B=" <<  B << "; C=" << C ;
	return 0;
}
3.	
#include <iostream>
#include <math.h>


using namespace std;
double A,B,C,k;

int main()
{
	setlocale(LC_ALL, "Russian");
	cout << "Введите A=";
	cin >> A;
	cout << "Введите B=";
	cin >> B;
	cout << "Введите C=";
	cin >> C;
	k = A;
	A = B;
	B = k;
	k = B;
	B = C;
	C = k;
	cout << "A=" << A << "; B=" <<  B << "; C=" << C ;
	return 0;
}

4.	
#include <iostream>
#include <math.h>


using namespace std;
double x, y;

int main()
{
	setlocale(LC_ALL, "Russian");
	cout << "Введите x=";
	cin >> x;
	y = 3 * pow(x, 6) - 6 * x * x - 7;
	cout << "y=" << y;
	return 0;
}

5.	
#include <iostream>
#include <math.h>


using namespace std;
double x, y;

int main()
{
	setlocale(LC_ALL, "Russian");
	cout << "Введите x=";
	cin >> x;
	y = 4 * pow(x-3, 6) - 7 * pow(x-3,3) + 2;
	cout << "y=" << y;
	return 0;
}

6.	
#include <iostream>
#include <math.h>


using namespace std;
double A, k, y;

int main()
{
	setlocale(LC_ALL, "Russian");
	cout << "Введите A=";
	cin >> A;
	k = pow(A, 2);
	y = k*k*k*k;
	cout << "A^8=" << y;
	return 0;
}
7.
#include <iostream>
#include <math.h>


using namespace std;
double A, k, l, y;

int main()
{
	setlocale(LC_ALL, "Russian");
	cout << "Введите A=";
	cin >> A;
	k = pow(A, 2);
	l = pow(A, 3);
	y = k*k*k*l*l*l;
	cout << "A^15=" << y;
	return 0;
}


